##########################################

TARGET := main

##########################################

CC := g++
RM-F := rm -f

##########################################



##########################################

all: $(TARGET)

main: Main.cpp
	$(CC) Main.cpp -o main

clean:
	$(RM-F) *.o
	$(RM-F) *.d

veryclean: clean
	$(RM-F) $(TARGET)

